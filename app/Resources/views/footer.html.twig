<footer class="">



    <div class="footer_info bg_dark">
        <div class="standard_wide c_white">
            <div uk-grid>

                <div class="uk-width-1-4@l">
                    <div>
                        <img class="logo" src="/assets/images/interface/logo.svg" alt="">
                    </div>
                    <div class="uk-margin-top">
                            <a class="c_grey" rel="nofollow" href="https://www.facebook.com/mix.rent.team/"><span uk-icon="icon:facebook"></span></a>
                            &nbsp;
                            {#<a class="c_grey" rel="nofollow" href="https://twitter.com/multiprokat"><span uk-icon="icon:twitter"></span></a>#}
                            {#&nbsp;#}
                            <a class="c_grey" rel="nofollow" href="https://plus.google.com/u/1/108392252184431209923"><span uk-icon="icon:google-plus"></span></a>
                            &nbsp;
                            {#<a class="c_grey" rel="nofollow" href="https://www.instagram.com/multiprokat/"><span uk-icon="icon:instagram"></span></a>#}
                            {#&nbsp;#}
                            {#<a class="c_grey" rel="nofollow" href="https://vk.com/multiprokatcom"><span class="fa fa-vk"></span></a>#}
                            {#&nbsp;#}
                            {#<a class="c_grey" rel="nofollow" href="https://www.youtube.com/channel/UCj-igu9KFIbzdzVCezy1qFg"><span uk-icon="icon:youtube"></span></a>#}
                        </div>

                    <div>
                        <div class="uk-text-small uk-margin-top c_grey">&copy; {{ date()|date("Y") }} {{ app.request.host }} | {% trans %}Сервис аренды автотранспорта{% endtrans %}</div>
                    </div>

                </div>

                <div class="uk-width-1-4@l">
                    <div class="uk-margin-bottom uk-text-uppercase"><b>{% trans %}Категории{% endtrans %}</b></div>
                    <a rel="nofollow" href="/rent/{{ city.url }}/all/cars" class="c_grey">{% trans %}Легковые автомобили{% endtrans %}</a>
                    <hr>
                    <a rel="nofollow" href="/rent/{{ city.url }}/all/wedding" class="c_grey">{% trans %}Свадебные авто{% endtrans %}</a>
                    <hr>
                    <a rel="nofollow" href="/rent/{{ city.url }}/all/helicopters" class="c_grey">{% trans %}Вертолеты{% endtrans %}</a>
                    <hr>
                    <a rel="nofollow" href="/rent/{{ city.url }}/all/snowride" class="c_grey">{% trans %}Снегоходы{% endtrans %}</a>
                    <hr>
                    <a rel="nofollow" href="/rent/{{ city.url }}/all/trucks" class="c_grey">{% trans %}Грузовые автомобили{% endtrans %}</a>
                </div>


                <div class="uk-width-1-4@l">
                    <div class="uk-margin-bottom uk-text-uppercase"><b>{% trans %}Информация{% endtrans %}</b></div>
                        <a rel="nofollow" href="/news" class="c_grey">{% trans %}Новости{% endtrans %}</a>
                        <hr>
                        <a rel="nofollow" href="/contacts" class="c_grey">{% trans %}Контакты{% endtrans %}</a>
                        <hr>

                        {% if page_type is defined and general is defined and general is not iterable%}
                            <a href="/card/new/{{ general.url }}" class="c_grey">Сдать в {{ general.url == 'special-machines' or general.url == 'airplanes' ? 'аренду' : 'прокат/аренду' }} {{ general.headerSingular|lower }} в {{ city.gde }}</a>
                        {% else %}
                            <a href="/promo" class="c_grey">{% trans %}Сдать свой автомобиль{% endtrans %}</a>
                        {% endif %}






                </div>

                <div class="uk-width-1-4@l uk-visible@l">
                    <div class="uk-margin-bottom uk-text-uppercase"><b>{% trans %}Новые на сайте{% endtrans %}</b></div>
                    {% for card in t3_new.getNew %}
                        {{ include(':footer:top_new_tpl.html.twig', {'card':card}) }}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>



