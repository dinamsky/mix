{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('assets/css/owl.carousel.min.css') }}" rel="stylesheet"/>
    <link href="{{ asset('assets/css/owl.theme.default.css') }}" rel="stylesheet"/>
    <link href="{{ asset('assets/css/auto-complete.css') }}" rel="stylesheet" />
    <link href="{{ asset('assets/css/datepicker.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('assets/css/main_page/top_slider.css') }}" rel="stylesheet"/>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('assets/js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('assets/js/city_general_mark_selector.js') }}"></script>
    <script src="{{ asset('assets/js/showCard.js') }}"></script>
    <script src="{{ asset('assets/js/sliderSettings.js') }}"></script>
    <script src="{{ asset('assets/js/auto-complete.min.js') }}"></script>
    <script src="{{ asset('assets/js/mark_autocomplete.js') }}"></script>
    <script src="{{ asset('assets/js/form_city_autocomplete.js') }}"></script>
    <script src="{{ asset('assets/js/recaptcha_settings.js') }}"></script>
    <script src="{{ asset('assets/js/recapthcha_validate.js') }}"></script>
    <script src="{{ asset('assets/js/datepicker.min.js') }}"></script>
    <script src="{{ asset('assets/js/chat.js') }}"></script>
{% endblock %}

{% block socialScripts %}
    <!-- VK -->
    <script type="text/javascript" src="//vk.com/js/api/openapi.js?149"></script>
    <script type="text/javascript">VK.init({apiId: 6213461, onlyWidgets: true});</script>
    <script type="text/javascript" src="https://vk.com/js/api/share.js?95" charset="windows-1251"></script>
    <!-- VK -->
    {#<script src='https://www.google.com/recaptcha/api.js'></script>#}
{% endblock %}


{% block openGraph %}
    {{ parent() }}
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ app.request.getSchemeAndHttpHost() ~ mainFoto }}">
    <meta property="og:title" content="{{ include(':seo_templates:p_h1.html.twig') }}">
    <meta property="og:url" content="{{ app.request.schemeAndHttpHost ~ app.request.requestUri }}">
{% endblock %}

{% block metaTags %}
    {{ parent() }}
    <meta name="description" content="{% include ':seo_templates:p_description.html.twig' %}">
{% endblock %}

{% block htmlblock %}
 prefix="og: http://ogp.me/ns#"
{% endblock %}

{% block title %}{{ include(':seo_templates:p_title.html.twig') }}{% endblock %}

{% block content %}

    <!-- FB -->
    {#<div id="fb-root"></div>#}
    {#<script>(function(d, s, id) {#}
            {#var js, fjs = d.getElementsByTagName(s)[0];#}
            {#if (d.getElementById(id)) return;#}
            {#js = d.createElement(s); js.id = id;#}
            {#js.src = "//connect.facebook.net/ru_RU/sdk.js#xfbml=1&version=v2.10&appId=1747516202216504";#}
            {#fjs.parentNode.insertBefore(js, fjs);#}
        {#}(document, 'script', 'facebook-jssdk'));</script>#}
    <!-- FB -->



    {% if app.session.has('share') %}
        {{ app.session.remove('share') }}
    {% endif %}

    <div class="standard_wide breadcrumbs uk-margin-top uk-margin-bottom">
        <a class="c_grey" href="/">{% trans %}Главная{% endtrans %}</a>
        <i uk-icon="icon:chevron-right"></i>
        <a class="c_grey" href="/rent/{{ card.city.url }}">{{ lang=='ru' ? card.city.header : card.city.url }}</a>
        <i uk-icon="icon:chevron-right"></i>
        <a class="c_grey" href="/rent/{{ card.city.url }}/all/{{ card.generalType.url }}">{{ lang == 'ru' ? card.generalType.header : card.generalType.url }}</a>

        {% if bodyType %}
            <i uk-icon="icon:chevron-right"></i>
            <a class="c_grey" href="/rent/{{ card.city.url }}/all/{{ card.generalType.url }}/{{ bodyType.url }}">{{ lang == 'ru' ? bodyType.header : bodyType.url }}</a>
        {% endif %}

        {% if card.modelId != 20991 %}
            <i uk-icon="icon:chevron-right"></i>
            <a class="c_grey" href="/rent/{{ card.city.url }}/all/{{ card.generalType.url }}/{{ card.markModel.mark.header }}">{{ card.serviceTypeId == 1 ? 'Прокат'|trans : 'Аренда'|trans }} {{ card.markModel.mark.header }} в {{ card.city.gde }}</a>
            <i uk-icon="icon:chevron-right"></i>
            <a class="c_grey" href="/rent/{{ card.city.url }}/all/{{ card.generalType.url }}/{{ card.markModel.mark.header }}/{{ card.markModel.header }}">{{ card.markModel.mark.header }} {{ card.markModel.header }}</a>
        {% endif %}
    </div>

    <div class="uk-position-relative">



        <div id="page_slider" class="owl-carousel owl-carousel-all owl-theme" data-items="1" data-full="1" data-dots="2">
            {% for k,foto in card.getFotos %}
                <div uk-lightbox>
                <a href="/assets/images/cards/{{ foto.folder }}/{{ foto.id }}.jpg" class="slide uk-width-1-1" style="
                        background:url('/assets/images/cards/{{ foto.folder }}/{{ foto.id }}.jpg') center center;
                        background-size: cover;
                        display: block;
                        height: {% if is_mobile() %}150px{% else %}600px{% endif %};
                        "
                {% if k == 0 %}
                title="{{ card.serviceTypeId == 1 ? 'Прокат' : 'Аренда' }} {% if card.generalType.chegoSingular is defined %}{{ card.generalType.chegoSingular }}{% else %}{% endif %} {{ card.markModel.Mark.header }} {{ card.markModel.header }} в {{ card.city.gde }}"
                {% endif %}
                >
                    {% if k == 0 %}
                    <img src="https://multiprokat.com/assets/images/cards/{{ foto.folder }}/{{ foto.id }}.jpg" alt="" style="visibility: hidden; height: 0;width: 0;" itemprop="image">
                    {% endif %}
                    {#<img src="/assets/images/cards/{{ foto.folder }}/{{ foto.id }}.jpg" alt="" style="max-height: auto">#}
                </a>
                </div>




            {% endfor %}
        </div>

        <div class="standard_wide uk-position-absolute" style="top: 0;left:0;right: 0;">
            <div class="likes" data-card_id="{{ card.id }}">
                {% if app.session.has('likes') and app.session.get('likes')[card.id] is defined %}
                    <i class="fa fa-heart c_red"></i>
                {% else %}
                    <i class="fa fa-heart-o c_white t_shadowed"></i>
                {% endif %}
            </div>

            {% if reserved %}
                <div class="page_reserved">Бронь до {{ card.dateRentFinish|date("d.m.Y") }}</div>
            {% endif %}
        </div>



    </div>



    <div class="standard_wide uk-margin-top">

        <div uk-grid itemscope itemtype="http://schema.org/Product">
            <div class="uk-width-3-4@s ">


                <div class="uk-grid" uk-grid>
                    <div class="uk-width-1-4 uk-text-right">{% trans %}Авто{% endtrans %}:</div>
                    <div class="uk-width-3-4"><h1 itemprop="name">{{ include(':seo_templates:p_h1.html.twig') }}</h1></div>
                </div>





                {% if card.getCardFeatures is not empty%}
                    <div class="uk-grid" uk-grid>
                        <div class="uk-width-1-4 uk-text-right">{% trans %}Характеристики{% endtrans %}:</div>
                        <div class="uk-width-3-4">
                            <div class="">

                                    {% for feature in card.getCardFeatures %}
                                        <div class="">
                                            <span class="check_icon"
                                                  uk-icon="icon:check; ratio: 1"></span>{{ feature.getFeature.header }}
                                        </div>
                                    {% endfor %}

                            </div>
                        </div>
                    </div>
                {% endif %}


                <div class="uk-grid" uk-grid>
                    <div class="uk-width-1-4 uk-text-right">{% trans %}Информация{% endtrans %}:</div>
                    <div class="uk-width-3-4">
                        <div class="">

                            {% if card.prodYear != 0 %}<div>{% trans %}Год выпуска{% endtrans %}: {{ card.prodYear }}</div>{% endif %}

                            <div>{% trans %}Состояние{% endtrans %}: {{ card.getCondition.header }}</div>

                            <div>{{ card.getColor != null ? 'Цвет'|trans~': '~card.getColor.header }}</div>


                            {% for subfield in sub_fields if subfield.value is not null %}
                                {% if subfield.type == 'subfield' %}
                                    <div>
                                        {{ subfield.label }}: {{ subfield.value.getHeader }}
                                    </div>
                                {% elseif subfield.value.getValue != 0 and subfield.value.getValue != '' %}
                                    <div>
                                        {{ subfield.label }}: {{ subfield.value.getValue }}
                                    </div>
                                {% endif %}

                            {% endfor %}

                            <div>{% trans %}Доставка до клиента{% endtrans %}:
                                {{ card.deliveryStatus == 0 ? 'нет, клиент забирает транспорт самостоятельно'|trans }}
                                {{ card.deliveryStatus == 1 ? 'да, мы доставляем транспорт'|trans }}
                                {{ card.deliveryStatus == 2 ? 'возможна, по согласованию'|trans }}
                            </div>

                        </div>
                        <div class="uk-margin-top">
                            <div id="card_content" itemprop="description">{{ card.content|striptags('<br><p>')|raw }}</div>
                            {% if card.content|length>300 %}
                            <div class="uk-text-small uk-text-center uk-margin-top show_full_content cursor-pointer">{% trans %}показать полностью{% endtrans %}</div>
                            {% endif %}
                        </div>
                    </div>
                </div>



                <div class="uk-grid" uk-grid>
                    <div class="uk-width-1-4 uk-text-right">{% trans %}Отзывы{% endtrans %}:</div>
                    <div class="uk-width-3-4">
                        {% if app.session.has('logged_user') %}
                            <form action="/userAddOpinion" method="POST">
                                <div class="stars uk-margin-bottom">
                                    {% for i in 1..5 %}
                                        <span class="star {{ i == 1 ? 'active' }}" data-star="{{ i }}"><i class="fa fa-star"></i></span>
                                    {% endfor %}
                                    <span id="stars">1</span>
                                </div>
                                <textarea name="content" class="uk-textarea" placeholder="{% trans %}Ваш отзыв{% endtrans %}" required></textarea>
                                <input type="hidden" name="stars" value="1">
                                <input type="hidden" name="user_id" value="{{ app.session.get('logged_user').id }}">
                                <input type="hidden" name="card_id" value="{{ card.id }}">
                                <input type="hidden" name="return" value="{{ app.request.requestUri }}">
                                <button class="uk-button uk-button-primary uk-margin-top">{% trans %}Отправить{% endtrans %}</button>
                            </form>

                        {% else %}
                            <div>
                                <a uk-toggle="target: #sign_in">{% trans %}Войдите{% endtrans %}</a> {% trans %}или{% endtrans %} <a uk-toggle="target: #sign_up">{% trans %}зарегистрируйтесь{% endtrans %}</a>, {% trans %}чтобы оставить отзыв{% endtrans %}.
                            </div>
                        {% endif %}

                        {% if card.opinions is not empty %}
                            <hr>
                            <div class="uk-margin-top">
                                {% for opinion in card.opinions %}
                                    <div class="uk-margin-bottom">
                                        <i class="fa fa-calendar"></i> {{ opinion.dateCreate|date('d.m.Y H:i') }}
                                        <i class="fa fa-user"></i> {{ opinion.user.header }}
                                        <i class="fa fa-star c_gold"></i> {{ opinion.stars }}
                                        <div><i class="fa fa-comment-o"></i> {{ opinion.content }}</div>
                                    </div>
                                {% endfor %}
                            </div>
                        {% endif %}
                    </div>
                </div>

                <div class="uk-grid" uk-grid>
                    <div class="uk-width-1-4 uk-text-right">{% trans %}Комментарии{% endtrans %}:</div>
                    <div class="uk-width-3-4">
                        {% if app.session.has('logged_user') %}
                            <form action="/userAddComment" method="POST">
                                <textarea name="comment" class="uk-textarea" placeholder="{% trans %}Ваш комментарий{% endtrans %}" required></textarea>
                                <input type="hidden" name="user_id" value="{{ app.session.get('logged_user').id }}">
                                <input type="hidden" name="card_id" value="{{ card.id }}">
                                <input type="hidden" name="return" value="{{ app.request.requestUri }}">
                                <button class="uk-button uk-button-primary uk-margin-top">{% trans %}Отправить{% endtrans %}</button>
                            </form>
                        {% else %}
                            <div>
                                <a uk-toggle="target: #sign_in">{% trans %}Войдите{% endtrans %}</a> {% trans %}или{% endtrans %} <a uk-toggle="target: #sign_up">{% trans %}зарегистрируйтесь{% endtrans %}</a>, {% trans %}чтобы написать комментарий{% endtrans %}.
                            </div>
                        {% endif %}

                        {% if card.comments is not empty %}
                            <hr>
                        <div class="uk-margin-top">
                            {% for comment in card.comments %}
                                <div class="uk-margin-bottom">
                                    <i class="fa fa-calendar"></i> {{ comment.dateCreate|date('d.m.Y H:i') }}
                                    <i class="fa fa-user"></i> {{ comment.user.header }}
                                    <div><i class="fa fa-comment-o"></i> {{ comment.content }}</div>
                                </div>
                            {% endfor %}
                        </div>
                        {% endif %}
                    </div>
                </div>



                <div class="uk-grid" uk-grid>
                    <div class="uk-width-1-4 uk-text-right">{% trans %}Похожие{% endtrans %}:</div>
                    <div class="uk-width-3-4">
                        <div uk-grid class="uk-grid-small uk-grid-match">
                            {% for card in similar %}
                                <div class="uk-width-1-2@s">
                                {{ include('search/one_grid_view.html.twig', {'card':card, 'height':'150px'}) }}
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                </div>

            </div>


{# -----------------------------------------------------------  This is right block !  ----------------------------------------------- #}


            <div class="uk-width-1-4@s">

                {% if not reserved %}
                <div class="uk-margin-bottom card_page_phone" {{ is_mobile() ? '' : 'uk-sticky="offset: 110"' }}>
                    <div uk-grid class="uk-grid-collapse uk-grid-match b_shadowed">
                        <div class="phone_block uk-width-1-1">
                            {% if app.session.get('phone')[card.user.id] is not defined %}
                                <div class="hidden_phone bg_blue show_phone uk-text-center c_white" data-card_id="{{ card.id }}" data-profile="0">
                                    <div class="uk-text-center">{% trans %}Показать телефон{% endtrans %}</div>
                                    <i class="fa fa-phone"></i> {% for info in card.user.information if info.uiKey == 'phone' %}{{ info.uiValue|slice(0,5) }} XXX-XX-XX{% endfor %}
                                </div>

                            {% else %}
                                <a class="opened_phone bg_green c_white uk-text-center" href="tel:{% for info in card.user.information if info.uiKey == 'phone' %}{{ info.uiValue }}{% endfor %}">
                                    <i class="fa fa-phone"></i> {% for info in card.user.information if info.uiKey == 'phone' %}{{ info.uiValue }}{% endfor %}
                                </a>
                            {% endif %}
                        </div>

                    </div>
                </div>
                {% endif %}

                <div class="bordered padding10">

                    <div class="" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                        <div class="uk-clearfix">
                            <div class="uk-float-left">
                                {% for price in card.getCardPrices if price.priceId == 2%}
                                    <div class="">
                                        <span class="price_value">{{ price.value|number_format(0,'.','<span class="thousand_separator"></span>')|raw }}</span>
                                        {% set currency = {
                                            'RUB': '₽',
                                            'EUR': '€',
                                            'USD': '$'
                                        } %}
                                        {% if price.priceId == 2%}
                                            <span itemprop="price" style="visibility: hidden;height: 0;width: 0;display: none;">{{ price.value }}</span>
                                            <span itemprop="priceCurrency" style="visibility: hidden;height: 0;width: 0;display: none;">{{ card.currency }}</span>
                                        {% endif %}
                                        <span class="price_descriptor uk-text-truncate">{{ currency[card.currency] }}/{{ price.getPrice.header|trans }}</span>
                                    </div>
                                {% endfor %}
                            </div>

                            <div class="uk-float-right uk-text-right">
                                <div uk-toggle="target: #share_butons"><i uk-icon="icon:social"></i></div>
                            </div>
                        </div>

                        <button class="uk-button uk-button-primary uk-width-1-1" uk-toggle="target: #user_book_form">{% trans %}Отправить заявку{% endtrans %}</button>
                    </div>

                </div>

                <div class="bordered padding10 uk-margin-top">
                    <div class="uk-text-small uk-margin-bottom">
                    {% trans %}Собственник{% endtrans %}: <a class="c_blue" href="/user/{{ card.user.id }}" class="uk-text-truncate">{{ card.user.header }}</a>
                    </div>
                    <div><i class="fa fa-map-marker c_blue"></i>
                        <span class="uk-text-small">
                            {% if card.address != '' %}
                                {{ card.address }}
                            {% else %}
                                {{ card.getCity.header }}, {{ card.getCity.getParent.header }}, {{ card.getCity.country }}
                            {% endif %}
                        </span>
                    </div>
                    <div><i class="fa fa-calendar c_blue"></i> {{ card.dateCreate|date("d.m.Y") }}</div>
                    <div><i class="fa fa-eye c_blue"></i> {{ card.views }} <span class="uk-text-small c_grey">{% trans %}просмотров{% endtrans %}</span></div>
                    <div><i class="fa fa-heart c_blue"></i> <span id="card_likes">{{ card.likes }}</span> <span class="uk-text-small c_grey">{% trans %}лайков{% endtrans %}</span></div>
                    <div>
                        <i class="fa fa-star c_blue"></i> {{ opinions }}/5 <span class="uk-text-small c_grey">{% trans %}звезд из{% endtrans %}</span> {{ total_opinions }}
                        <span class="uk-text-small c_grey">{% trans %}отзывов{% endtrans %}</span>
                    </div>
                    <hr>
                    <div class="uk-text-center"><a class="c_blue" href="/user/{{ card.user.id }}">{% trans %}Все объявления{% endtrans %} {% trans %}пользователя{% endtrans %} ({{ card.user.cards|length }})</a></div>

                    {% if app.session.has('logged_user') and blockings[app.session.get('logged_user').id] is not defined%}
                        <div class="open_chat cursor-pointer uk-text-center" data-user_id="{{ card.user.id }}" data-visitor_id="{{ app.session.get('logged_user').id }}" data-card_id="{{ card.id }}">{% trans %}Написать сообщение{% endtrans %}</div>
                        {{ include('user/chat_modal.html.twig') }}
                    {% endif %}
                </div>




                <div id="user_message_form" uk-modal>
                    <div class="uk-modal-dialog uk-modal-body">
                        <h2 class="uk-modal-title">{% trans %}Сообщение пользователю{% endtrans %}</h2>
                        <button class="uk-modal-close-default" type="button" uk-close></button>
                        <form action="/user/sendMessage" uk-grid class="uk-grid-small" method="POST" >
                            <div class="uk-width-1-1"><input type="text" name="name" class="uk-input" placeholder="{% trans %}Имя{% endtrans %}"></div>
                            <div class="uk-width-1-1"><input type="text" name="phone" class="uk-input uk-width-1-1" placeholder="{% trans %}Телефон{% endtrans %}"></div>
                            <div class="uk-width-1-1"><input type="text" name="email" class="uk-input" placeholder="Email" required></div>
                            <div class="uk-width-1-1"><textarea name="message" class="uk-width-1-1 short_textarea" placeholder="{% trans %}Сообщение{% endtrans %}" required></textarea>
                            </div>
                            <input type="hidden" name="card_id" value="{{ card.id }}">
                            <div class="uk-width-1-1">
                                <button class="uk-button uk-button-default uk-width-1-1">{% trans %}Отправить{% endtrans %}</button>
                            </div>
                            {#<div id="recap_1" class="g-recaptcha"#}
                                 {#data-callback="capcha_filled"#}
                                 {#data-expired-callback="capcha_expired"#}
                                 {#data-sitekey="6LcGCzUUAAAAADpeaCQhkXWZqdhnB6_ZEGRm7Z2m"></div>#}
                            <input type="hidden" name="g-recaptcha-response">
                            <div id="recap1"></div>
                        </form>
                    </div>
                </div>

                <div id="user_book_form" uk-modal="bg-close:false" data-res="{{ reserved ? card.dateRentFinish|date_modify("+1 day")|date('Y/m/d') : "now"|date("Y/m/d") }}">
                    <div class="uk-modal-dialog uk-modal-body">
                        <h2 class="uk-modal-title">{% trans %}Заявка{% endtrans %}</h2>
                        <button class="uk-modal-close-default" type="button" uk-close></button>
                        <form action="/user/bookMessage" uk-grid class="uk-grid-small" method="POST" >

                            <div class="uk-width-1-1">
                                <span class="uk-text-small">{% trans %}Транспортное средство{% endtrans %}</span>
                                <input type="text" name="header" class="uk-input" value="{{ card.markModel.mark.header }} {{ card.markModel.header }}">
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">{% trans %}Дата приема{% endtrans %} <span class="c_red">{{ reserved ? 'с '~card.dateRentFinish|date_modify("+1 day")|date('d.m.Y') }}</span></span>
                                <input type="text" name="date_in" class="uk-input uk-width-1-1 datepicker-reserve" >
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">{% trans %}Дата возврата{% endtrans %} <span class="c_red">{{ reserved ? 'с '~card.dateRentFinish|date_modify("+1 day")|date('d.m.Y') }}</span></span>
                                <input type="text" name="date_out" class="uk-input uk-width-1-1 datepicker-reserve">
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">{% trans %}Город приема{% endtrans %}</span>
                                <input type="text" name="city_in" class="uk-input uk-width-1-1" value="{{ card.city.header }}">
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">{% trans %}Город возврата{% endtrans %}</span>
                                <input type="text" name="city_out" class="uk-input uk-width-1-1" value="{{ card.city.header }}">
                            </div>

                            <div class="uk-width-1-1">
                                <span class="uk-text-small">{% trans %}Имя и фамилия{% endtrans %}</span>
                                <input type="text" name="full_name" class="uk-input uk-width-1-1" value="{{ app.session.get('logged_user') ? app.session.get('logged_user').header }}">
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">Email</span>
                                <input type="text" name="email" class="uk-input uk-width-1-1" value="{{ app.session.get('logged_user') ? app.session.get('logged_user').email }}">
                            </div>

                            <div class="uk-width-1-2">
                                <span class="uk-text-small">{% trans %}Телефон{% endtrans %}</span>
                                <input type="text" name="phone" class="uk-input uk-width-1-1" value="{% if app.session.has('logged_user') %}{% for info in app.session.get('logged_user').information if info.uiKey == 'phone' %}{{ info.uiValue }}{% endfor %}{% endif %}">
                            </div>


                            <div class="uk-width-1-1">
                                <span class="uk-text-small">{% trans %}Ваш коментарий к заявке{% endtrans %}</span>
                                <textarea name="alternative" class="uk-width-1-1 short_textarea"></textarea>
                            </div>

                            <input type="hidden" name="card_id" value="{{ card.id }}">

                            <div class="uk-width-1-1">
                                <button class="uk-button uk-button-default uk-width-1-1">{% trans %}Отправить{% endtrans %}</button>
                            </div>

                            <input type="hidden" name="g-recaptcha-response">
                            <div id="recap3"></div>
                        </form>
                    </div>
                </div>

                <div id="user_phone_form" uk-modal>
                    <div class="uk-modal-dialog uk-modal-body">
                        <button class="uk-modal-close-default" type="button" uk-close></button>
